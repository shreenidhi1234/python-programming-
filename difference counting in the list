#difference counting in the list

n = int(input("enter the range of the list:"))

l = [ ]

for i in range(n):

    n1 = int(input("enter the elements in the list"))

    l.append(n1)

lst = [ ]

for i in l:

    count = 0

    for j in l:

        if(j<i):

            count+=1

    lst.append(count)

print(l)

print(lst)
